/*******************************
     User Global Variables
*******************************/
@headerFont        : 'Magra', 'Helvetica Neue', Arial, Helvetica, sans-serif;
@pageFont          : 'Magra', 'Helvetica Neue', Arial, Helvetica, sans-serif;
@fontSmoothing     : antialiased;

@importGoogleFonts : true;
@googleFontName    : 'Magra';
@googleFontSizes   : '400,700,400italic,700italic';
